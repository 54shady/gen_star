# CROSS_COMPILE
CP="/home/sam/rk3368/rk3368_android51_repo/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/bin/aarch64-linux-android-"

# some config and prebuild files
mkdir -p out/tools/rk_tools/RKBOOT
mkdir -p out/tools/rk_tools/bin/rk33
mkdir -p out/tools/rk_tools/RKTRUST

cp tools/rk_tools/RKBOOT/RK3368MINIALL.ini out/tools/rk_tools/RKBOOT/
cp tools/rk_tools/RKTRUST/RK3368TRUST.ini out/tools/rk_tools/RKTRUST/
cp tools/rk_tools/bin/rk33/RK3368_DDR_800M_V1.16.bin out/tools/rk_tools/bin/rk33/
cp tools/rk_tools/bin/rk33/rk3368usbplug.bin out/tools/rk_tools/bin/rk33/
cp tools/rk_tools/bin/rk33/rk3368miniloaderall_v2.40.bin out/tools/rk_tools/bin/rk33/
cp tools/rk_tools/bin/rk33/rk3368bl30_v2.12.bin  out/tools/rk_tools/bin/rk33/
cp tools/rk_tools/bin/rk33/rk3368bl31_v1.7.bin out/tools/rk_tools/bin/rk33/

make ARCHV=aarch64 CROSS_COMPILE=$CP rk3368_defconfig O=out
make ARCHV=aarch64 CROSS_COMPILE=$CP O=out -j32
